generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url = "mysql://root:root@localhost:3306/modslauncher"
}

model User {
    id Int @id
    name String
    email String
    password String
    token String
    createdAt DateTime @default(now())
    updatedAt DateTime @default(now())
}

model Mod {
    id Int @id
    name String
    description String
    package String
    version String
    icon String
    createdAt DateTime @default(now())
    updatedAt DateTime @default(now())
}