generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url = "mysql://root:root@localhost:3306/modslauncher"
}

model User {
    id Int @id @default(autoincrement())
    email String
    password String
    createdAt DateTime @default(now())
    updatedAt DateTime @default(now())
}

model Mod {
    id Int @id @default(autoincrement())
    name String
    description String
    package String
    modPackage String
    version String
    modVersion String
    icon String
    createdAt DateTime @default(now())
    updatedAt DateTime @default(now())
}

model Invite {
    id Int @id @default(autoincrement())
    code String
    userId Int
    createdAt DateTime @default(now())
    updatedAt DateTime @default(now())
}